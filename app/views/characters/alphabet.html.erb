<% content_for :title, @title %>

<% content_for :canonical, @canonical %>

<% content_for :heading, "Characters by Alphabet #{@alphabet}" %>

<% content_for :content do %>
  <div class="row">
    <% @characters.each do |character| %>
      <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
        <div class="more-link">
          <%= link_to character.name, character_url(character, format: :html), title: character.name + " Quotes" %>
        </div>
      </div>
    <% end %>
  </div>
<% end %>
