<div class="col-sm-12">
  <h2 style="margin-top: 0px">Tweetable Quotes - <%= @tweetable_quotes.count %></h2>
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th width="90%" class="text-center">Quotes</th>
        <th width="10%" class="text-center">Manage</th>
      </tr>
    </thead>
  
    <tbody>
      <% @tweetable_quotes.each do |tq| %>
        <tr>
          <td><%= tq.quote.text %></td>
          <td class="text-center"><%= link_to '<i class="fa fa-times"></i>'.html_safe, admin_tweetable_quote_url(tq), method: :delete, class: "delete-quote" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <div class="text-center">
    <%= paginate @tweetable_quotes, :views_prefix => 'kaminari_bootstrap' %>
  </div>
</div>