<script>
    bqAuctionRan = false;
    try {
        googletag.cmd.push(function () {
            
            SLOTS['div-gpt-ad-1418667263920-5'] = slot = googletag.defineSlot('/1008298/BQ_ros_top_728x90', [[728, 90],[970, 250]], 'div-gpt-ad-1418667263920-5').addService(googletag.pubads());
            
            SLOTS['div-gpt-ad-1418667263920-4'] = slot = googletag.defineSlot('/1008298/BQ_ros_square_300x250', [[300, 250],[1, 1]], 'div-gpt-ad-1418667263920-4').addService(googletag.pubads());
            
            SLOTS['div-gpt-ad-1418667263920-3'] = slot = googletag.defineSlot('/1008298/BQ_ros_middle_300x250', [[300, 250],[1, 1]], 'div-gpt-ad-1418667263920-3').addService(googletag.pubads());
            
            SLOTS['div-gpt-ad-1418667263920-2'] = slot = googletag.defineSlot('/1008298/BQ_ros_center_300x250', [[300, 250],[300, 600],[1, 1]], 'div-gpt-ad-1418667263920-2').addService(googletag.pubads());
            
            SLOTS['div-gpt-ad-1418667263920-1'] = slot = googletag.defineSlot('/1008298/BQ_ros_bottom_300x250', [[300, 250],[1, 1]], 'div-gpt-ad-1418667263920-1').addService(googletag.pubads());
            
        });
    }
    catch (err) {
            BQErrorLog.logError("define slots err ", err);
    }

    pf.setAuctionProvider({
        name: 'Google',
        libUri: '//www.googletagservices.com/tag/js/gpt.js',
        init: function(targeting, done) {

        try {
            googletag.cmd.push(function () {
                var i;

                for (i = 0; i < targeting.length; i++) {
                    var tgtObject = targeting[i];
                    if (tgtObject.name) {
                        var gptslot = SLOTS[tgtObject.elementId];
                        if (!(typeof gptslot === "undefined")) {
                            for (var p in tgtObject.targeting) {
                                var targetVal = '' + tgtObject.targeting[p];
                                gptslot.setTargeting(p, targetVal);
                                
                            }
                        }
                        else {
                            konsole.log('Targeting error, no slot found for slot ' + tgtObject.elementId);
                        }
                    } else {
                        for (key in tgtObject.targeting) {
                            var targetVal = '' + tgtObject.targeting[key];
                            
                            googletag.pubads().setTargeting(key, targetVal);
                        }
                    }
                }

                
                
                var stickySlot = SLOTS['div-gpt-ad-1418667263920-2'];
                if (!(typeof stickySlot === "undefined")) {
                    if (stickyAdHelper.isStickyOn()) {
                        stickySlot.setTargeting('format', 'sticky');
                    }
                }
                else {
                    konsole.log('Sticky ad error, no slot found for slot div-gpt-ad-1418667263920-2');
                }
                
                


                for (var bktDivId in bkts) {
                    var gptslot = SLOTS[bktDivId];
                    var targetVal = '' + bkts[bktDivId];
                    gptslot.setTargeting('bq_pcr',targetVal);
                }
                
                
                    googletag.pubads().setTargeting("test_group", "atest");
                
                googletag.pubads().setTargeting("bq_vide", "" + bqPd);
                
                googletag.pubads().setTargeting("bq_ct", "a");
                
                googletag.pubads().setTargeting("bq_aId", "155086");
                
                googletag.pubads().setTargeting("bq_pg", "l");
                
                if (CookieUtil.hasLI()) {
                    googletag.pubads().setTargeting('bq_m', 'y');
                } else {
                    googletag.pubads().setTargeting('bq_m', 'n');
                }
                googletag.pubads().setTargeting('bq_mbl', bqMbl);

                
                googletag.pubads().setTargeting("arch", "pf");

                while (bqTargetCmd.length > 0) {
                    (bqTargetCmd.shift())();
                }
                googletag.pubads().enableAsyncRendering();
                googletag.pubads().enableSingleRequest();
                googletag.pubads().collapseEmptyDivs();
                googletag.enableServices();
                bqAuctionRan = true;
                JsLOG.push("Google auction done");
                done();
            });
    }
    catch (err) {
        BQErrorLog.logError("google auction ", err);
    }

    },
    refresh: function(targeting, done) {
    }
    });

</script>
<script type="text/javascript">
    var OX_cmds = window.OX_cmds || [];
    OX_cmds.push(function() {
        try {
            var oAR_aV, oAR;
            if (oAR = (OX && OX.AdRequest)) {
                OX.AdRequest = function() {
                    oAR.apply(this, arguments);
                    oAR_aV = this.addVariable;
                    this.addVariable = function() {
                        var args = arguments,
                                key = args[0],
                                cvWhitelist = ["topics"];
                        if (cvWhitelist.indexOf(key) !== -1)
                        { oAR_aV.apply(this, arguments);
                        }
                    }
                };
                OX.prototype = oAR.prototype;
            }
        }
        catch (err) {
            BQErrorLog.logError("openx error  ", err);
        }
    });

    var openXDone = function () { };

    OX_dfp_options = {
        'callback': function() {
            OX.dfp_bidder.setOxTargeting();
            openXDone();
            
        }
    };
    OX_dfp_ads = []

    function szAryToStrAry(szAry) {
        var szStrAry = [];
        for (var i = 0; i < szAry.length; i++) {
            try {
                szStrAry.push(szAry[i][0]+'x'+szAry[i][1]);
            }
            catch (err) {
                BQErrorLog.logError("openx sz err  ", err);
            }
        }
        return szStrAry;
    }
    
    var szAryStr = szAryToStrAry([[728, 90],[970, 250]]);
    OX_dfp_ads.push(['/1008298/BQ_ros_top_728x90',szAryStr,'div-gpt-ad-1418667263920-5']);
    
    var szAryStr = szAryToStrAry([[300, 250],[1, 1]]);
    OX_dfp_ads.push(['/1008298/BQ_ros_square_300x250',szAryStr,'div-gpt-ad-1418667263920-4']);
    
