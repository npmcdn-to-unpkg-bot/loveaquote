<script type="text/javascript">

    var slotsByDivs = function(slts) {
        var slotsByDivMap = {};
        for (var k = 0; k < slts.length; k++) {
            slotsByDivMap[slts[k].elementId] = slts[k];
        }
        return slotsByDivMap
    };

    var snSlotMap = {};
    var snPushBid = function(b) {};
    var snDone = function() {};

    try {
        pf.addBidProvider({
            name: 'sonobi',
            libUri: ' ',
            init: function(slots, pushBid, done) {
                snSlotMap = slotsByDivs(slots);
                snPushBid = pushBid;
                snDone = done;
            },
            refresh: function(slots, pushBid, done) {
            }
        });
    }
    catch (err) {
        BQErrorLog.logError("sonobi failed  ", err);
    }

    window.load_trinity = function (sncb) {
        var assoc = {};
        
        assoc['div-gpt-ad-1418667263920-5'] ='e65b1bad2707e45213fa';
        assoc['div-gpt-ad-1418667263920-4'] ='b973c44cfac9fdce1c6b';
        assoc['div-gpt-ad-1418667263920-3'] ='5a3a2e1ccad49807cd41';
        assoc['div-gpt-ad-1418667263920-2'] ='302e70c6c092223ac04e';
        assoc['div-gpt-ad-1418667263920-1'] ='ae529ee4dc67ebf94e4d';

        var sbi_script = document.createElement('script');
        sbi_script.async = true;
        sbi_script.type = 'text/javascript';
        sbi_script.src = '//apex.go.sonobi.com/trinity.js?key_maker=' + JSON.stringify(assoc) + '&s=' + Math.floor(Math.random() * 1000);

        if (sbi_script.readyState) {
            sbi_script.onreadystatechange = function () {
                if (sbi_script.readyState == "loaded" || sbi_script.readyState == "complete") {
                    sbi_script.onreadystatechange = null;
                    sncb();
                }
            };
        } else {
            sbi_script.onload = function () {
                sncb();
            };
        }
        var node = document.getElementsByTagName('script')[0];
        node.parentNode.insertBefore(sbi_script, node);
    };

    var bidCb = function(){
        var sbiDc = window.sbi_dc;
        if (sbiDc) {
            var snBid = { targeting: { 'sbi_dc' : window.sbi_dc } };
            snPushBid(snBid);
        }

        var divAry = [];
        divAry.push('div-gpt-ad-1418667263920-5');
        divAry.push('div-gpt-ad-1418667263920-4');
        divAry.push('div-gpt-ad-1418667263920-3');
        divAry.push('div-gpt-ad-1418667263920-2');
        divAry.push('div-gpt-ad-1418667263920-1');
        

        for (var d = 0; d < divAry.length; d++) {
            var divId = divAry[d];
            var snSlot = snSlotMap[divId];
            if (snSlot) {
                var sltTgt = window.sbi_trinity[divId];
                if (sltTgt) {
                    var snCpm = 0.0;
                    var snCpmVal = sltTgt['sbi_mouse'];
                    if (!isNaN(snCpmVal)) { 
                        snCpm = Math.floor(snCpmVal * 10.0)/10.0;
                        snCpm = Math.min(snCpm,20.0);
                        // Replace with this after Sonobi fixes the response
                        //snCpm = Math.min(snCpmVal,20.0);
                        sltTgt['sbi_mouse'] = snCpm;
                    }
                    snBid = { slot: snSlot.name, value: snCpm, sizes: snSlot.sizes, targeting: sltTgt };
                    snPushBid(snBid);
                }
            }
            else {
                konsole.log("no slot found for divId",divId);
            }
        }
        snDone();
    };
    load_trinity(bidCb);
</script>
