<script src="https://aka-cdn.adtechus.com/dt/common/DAC.js"></script>
<script type="text/javascript">
    var aolDone = function () {};
    var aolPushBid = function (bid) {};
    var aolSlots = [];
    var aolDoneCount = 0;

    function pubTagLibHandler(response) {
        try {
            if (!bqAuctionRan) {
                aolDoneCount++;
                var config = response.gpt;
                var slotName = response.adContainerId;
                var cpm = response.cpm;

                if (!response.refreshed || (cpm == null && !config.slot)) {
                    var pfSlot = 'undefined_slot';
                    for (var k = 0; k < aolSlots.length; k++) {
                        if (slotName == aolSlots[k].elementId) {
                            pfSlot = aolSlots[k];
                        }
                    }
                    if (cpm != null) {
                        var aolT = {};
                        aolT[response.bidKey] = [cpm];
                        aolT[response.mpAliasKey] = [response.alias];
                        var bidObject = {
                            slot: pfSlot.name,
                            value: cpm,
                            sizes: pfSlot.sizes,
                            targeting: aolT
                        };
                        aolPushBid(bidObject);
                    }
                }

                if (aolDoneCount >= aolTotal) {
                    aolDone();
                }
            }
            else {
                JsLOG.push('skipped late aol bid');
                konsole.log('aol bid came in after auction ran');
            }
            
        }
        catch (err) {
            JsLOG.push('error with bid response ' + response.alias + ' c:' + response.cpm);
            
            aolDone();
        }
    }

    var bidRequestConfig = {
        server: 'adserver.adtechus.com',
        network: '10289.1',
        bidKey: 'aolbid',
        loadAd: pubTagLibHandler,
        loadAdTimeout: 2000,
        adjustment:  -20,
        formatCpmToDecimal: false,
        roundingConfig: [
            {from: 0,  to: 2000, roundFunction: 'tenCentsRound'},
            {from: 2001, to: -1, roundValue: 2000}
        ]
    };

    JsLOG.push("aol adunit config [start]");
    var dacBidRequestConfigs = [];
    var isHTTPS = ('https:' == document.location.protocol);
    
    
    dacBidRequestConfigs.push({
        placement: 3762278,
        alias: 'aol_ros_top',
        adContainerId: 'div-gpt-ad-1418667263920-5',
        secure: isHTTPS,
        performScreenDetection: false,
        gpt: {
            adUnitPath: '/1008298/BQ_ros_top_728x90',
            size: [728, 90]
        }
    });
    
    
    
    dacBidRequestConfigs.push({
        placement: 3762283,
        alias: 'aol_ros_square',
        adContainerId: 'div-gpt-ad-1418667263920-4',
        secure: isHTTPS,
        performScreenDetection: false,
        gpt: {
            adUnitPath: '/1008298/BQ_ros_square_300x250',
            size: [300, 250]
        }
    });
    
    
    
    dacBidRequestConfigs.push({
        placement: 3762282,
        alias: 'aol_ros_middle',
        adContainerId: 'div-gpt-ad-1418667263920-3',
        secure: isHTTPS,
        performScreenDetection: false,
        gpt: {
            adUnitPath: '/1008298/BQ_ros_middle_300x250',
            size: [300, 250]
        }
    });
    
    
    
    dacBidRequestConfigs.push({
        placement: 3762279,
        alias: 'aol_ros_center',
        adContainerId: 'div-gpt-ad-1418667263920-2',
        secure: isHTTPS,
        performScreenDetection: false,
        gpt: {
