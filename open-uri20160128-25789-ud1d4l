    try {
        pf.addBidProvider({
            name: 'indexExchange',
            libUri: ' ',
            init: function(slots, pushBid, done) {

                var onDoneIdxExch = function(){
                    googletag.cmd.push(function() {
                        done();
                    });
                };

                (function () {
                    var aplSrc = window.location.protocol === 'https:' ? 'https://js-sec.indexww.com/ht/brainyquote.js' : 'http://js.indexww.com/ht/brainyquote.js';
                    var scriptTag = document.createElement("script");
                    scriptTag.setAttribute("src", aplSrc);
                    scriptTag.setAttribute("type", "text/javascript");
                    scriptTag.onload = onDoneIdxExch;
                    scriptTag.async = true;
                    var firstScript = document.getElementsByTagName('script')[0];
                    if (firstScript.parentNode)
                    { firstScript.parentNode.insertBefore(scriptTag, firstScript); }
                })();
            },
            refresh: function(slots, pushBid, done) {
            }
        });
    }
    catch (err) {
        BQErrorLog.logError("adding ie failed  ", err);
    }
</script>
